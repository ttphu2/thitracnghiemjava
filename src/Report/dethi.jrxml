	<queryString>
		<![CDATA[SELECT
     cauhoi.`macauhoi` AS cauhoi_macauhoi,
     cauhoi.`hinhanh` AS cauhoi_hinhanh,
     cauhoi.`nghe` AS cauhoi_nghe,
     cauhoi.`cauhoi` AS cauhoi_cauhoi,
     cauhoi.`doanvan` AS cauhoi_doanvan,
     cauhoi.`dapan1` AS cauhoi_dapan1,
